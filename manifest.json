{
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "strict_min_version": "56.0",
      "id": "myshows@flytaly"
    }
  },
  "default_locale": "en",
  "description": "__MSG_description__",
  "name": "__MSG_name__",
  "version": "1.0",
  "homepage_url": "https://localhost",
  "icons": {
    "48": "/images/icon-48.png",
    "96": "/images/icon-96.png"
  },
  "permissions": [
    "identity",
    "storage",
    "alarms",
    "https://*.myshows.me/*"
  ],
  "background": {
    "scripts": [
      "scripts/config.js",
      "scripts/app.js",
      "scripts/types.js",
      "scripts/storage.js",
      "scripts/rpc-handler.js",
      "scripts/state.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_title": "__MSG_extension_title__",
    "default_icon": "/images/icon.svg",
    "default_popup": "popup/popup.html",
    "browser_style": true
  },
  "options_ui": {
    "page": "options/options.html"
  }
}
