{
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_description__",
  "name": "__MSG_name__",
  "version": "1.0",
  "homepage_url": "https://localhost",
  "icons": {
  },
  "permissions": [
    "identity",
    "storage",
    "alarms",
    "https://*.myshows.me/*"
  ],
  "background": {
    "scripts": [
      "scripts/config.js",
      "scripts/app.js",
      "scripts/types.js",
      "scripts/storage.js",
      "scripts/rpc-handler.js",
      "scripts/state.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_title": "__MSG_extension_title__",
    "default_popup": "popup/popup.html",
    "browser_style": true
  }
}
