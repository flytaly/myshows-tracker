{
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "strict_min_version": "63.0",
      "id": "myshows@flytaly"
    }
  },
  "default_locale": "en",
  "description": "__MSG_description__",
  "name": "__MSG_name__",
  "version": "1.2.2",
  "homepage_url": "https://github.com/flytaly/myshows-tracker",
  "icons": {
    "48": "/images/icon-48.png",
    "64": "/images/icon-64.png",
    "96": "/images/icon-96.png"
  },
  "permissions": [
    "identity",
    "storage",
    "alarms",
    "https://*.myshows.me/*"
  ],
  "background": {
    "scripts": [
      "bundles/background.js"
    ]
  },
  "browser_action": {
    "default_title": "__MSG_extension_title__",
    "default_icon": "/images/icon.svg",
    "default_popup": "popup.html",
    "browser_style": true
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  }
}
